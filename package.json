{
  "name": "ema-admin",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.11.0",
    "flow-bin": "^0.68.0",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "reason-react": "^0.3.4"
  },
  "devDependencies": {
    "bulma": "^0.6.2",
    "cross-env": "^5.1.4",
    "jest": "^22.4.2",
    "node-sass-chokidar": "^0.0.3",
    "ramda": "^0.25.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "serve:dev": "cross-env REACT_APP_ENV=dev npm-run-all -p watch-css start-js",
    "serve:qa": "cross-env REACT_APP_ENV=qa npm-run-all -p watch-css start-js",
    "serve:uat": "cross-env REACT_APP_ENV=uat npm-run-all -p watch-css start-js",
    "serve:prod": "cross-env REACT_APP_ENV=prod npm-run-all -p watch-css start-js",
    "build:dev": "npm run build-css && react-scripts build",
    "build:qa": "npm run build-css && react-scripts build",
    "build:uat": "npm run build-css && react-scripts build",
    "build:prod": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "flow": "flow",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
